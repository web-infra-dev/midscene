{
  "name": "@midscene/shared",
  "version": "0.12.4",
  "repository": "https://github.com/web-infra-dev/midscene",
  "homepage": "https://midscenejs.com/",
  "types": "./src/index.ts",
  "type": "module",
  "main": "./dist/lib/index.js",
  "module": "./dist/es/index.js",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./dist/lib/index.js",
      "import": "./dist/es/index.js"
    },
    "./constants": {
      "types": "./src/constants/index.ts",
      "default": "./dist/lib/constants/index.js",
      "import": "./dist/es/constants/index.js"
    },
    "./fs": {
      "types": "./src/node/fs.ts",
      "default": "./dist/lib/node/fs.js",
      "import": "./dist/es/node/fs.js"
    },
    "./img": {
      "types": "./src/img/index.ts",
      "default": "./dist/lib/img/index.js",
      "import": "./dist/es/img/index.js"
    },
    "./browser/img": {
      "types": "./src/img/index.ts",
      "default": "./dist/browser/img/index.js",
      "import": "./dist/browser/img/index.js"
    },
    "./utils": {
      "types": "./src/utils.ts",
      "default": "./dist/lib/utils/index.js",
      "import": "./dist/es/utils/index.js"
    },
    "./extractor": {
      "types": "./src/extractor/index.ts",
      "default": "./dist/lib/extractor/index.js",
      "import": "./dist/es/extractor/index.js"
    },
    "./extractor-debug": {
      "types": "./src/extractor/debug.ts",
      "default": "./dist/lib/extractor-debug/index.js",
      "import": "./dist/es/extractor-debug/index.js"
    },
    "./keyboard-layout": {
      "types": "./src/us-keyboard-layout.ts",
      "default": "./dist/lib/us-keyboard-layout/index.js",
      "import": "./dist/es/us-keyboard-layout/index.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": ["./src/index.ts"],
      "constants": ["./src/constants/index.ts"],
      "img": ["./src/img/index.ts"],
      "browser/img": ["./src/img/index.ts"],
      "fs": ["./src/node/fs.ts"],
      "utils": ["./src/utils.ts"],
      "extractor": ["./src/extractor/index.ts"],
      "extractor-debug": ["./src/extractor/debug.ts"],
      "keyboard-layout": ["./src/us-keyboard-layout.ts"]
    }
  },
  "files": ["dist", "src", "README.md"],
  "scripts": {
    "dev": "modern dev",
    "build": "npm run build:pkg && npm run build:script",
    "build:pkg": "modern build -c ./modern.config.ts",
    "build:script": "modern build -c ./modern.inspect.config.ts",
    "build:watch": "modern build -w",
    "reset": "rimraf ./**/node_modules",
    "lint": "modern lint",
    "bump": "modern bump",
    "pre": "modern pre",
    "change-status": "modern change-status",
    "gen-release-note": "modern gen-release-note",
    "release": "modern release",
    "new": "modern new",
    "upgrade": "modern upgrade",
    "prepublishOnly": "npm run build",
    "test": "vitest --run",
    "test:ai": "AITEST=true npm run test"
  },
  "dependencies": {
    "jimp": "0.22.12"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@modern-js/module-tools": "2.60.6",
    "typescript": "~5.0.4",
    "@types/node": "^18.0.0",
    "rimraf": "~3.0.2",
    "vitest": "3.0.5",
    "js-sha256": "0.11.0"
  },
  "sideEffects": [],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "exports": {
      ".": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/lib/index.js",
        "import": "./dist/es/index.js"
      },
      "./constants": {
        "types": "./dist/types/constants.d.ts",
        "default": "./dist/lib/constants/index.js",
        "import": "./dist/es/constants/index.js"
      },
      "./fs": {
        "types": "./dist/types/node/fs.d.ts",
        "default": "./dist/lib/node/fs.js",
        "import": "./dist/es/node/fs.js"
      },
      "./img": {
        "types": "./dist/types/img.d.ts",
        "default": "./dist/lib/img/index.js",
        "import": "./dist/es/img/index.js"
      },
      "./browser/img": {
        "types": "./dist/types/browser/img.d.ts",
        "default": "./dist/browser/img/index.js",
        "import": "./dist/browser/img/index.js"
      },
      "./utils": {
        "types": "./dist/types/utils.d.ts",
        "default": "./dist/lib/utils/index.js",
        "import": "./dist/es/utils/index.js"
      },
      "./extractor": {
        "types": "./dist/types/extractor/index.d.ts",
        "default": "./dist/lib/extractor/index.js",
        "import": "./dist/es/extractor/index.js"
      },
      "./extractor-debug": {
        "types": "./dist/types/extractor/debug.d.ts",
        "default": "./dist/lib/extractor-debug/index.js",
        "import": "./dist/es/extractor/debug.js"
      },
      "./keyboard-layout": {
        "types": "./dist/types/us-keyboard-layout.d.ts",
        "default": "./dist/lib/us-keyboard-layout/index.js",
        "import": "./dist/es/us-keyboard-layout/index.js"
      }
    },
    "typesVersions": {
      "*": {
        ".": ["./dist/types/index.d.ts"],
        "constants": ["./dist/types/constants.d.ts"],
        "img": ["./dist/types/img.d.ts"],
        "browser/img": ["./dist/types/browser/img.d.ts"],
        "fs": ["./dist/types/node/fs.d.ts"],
        "utils": ["./dist/types/utils.d.ts"],
        "extractor": ["./dist/types/extractor/index.d.ts"],
        "extractor-debug": ["./dist/types/extractor/debug.d.ts"],
        "keyboard-layout": ["./dist/types/us-keyboard-layout.d.ts"]
      }
    }
  }
}
