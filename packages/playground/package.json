{
  "name": "@midscene/playground",
  "version": "0.28.4",
  "description": "Midscene playground utilities for web integration",
  "author": "midscene team",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "browser": {
        "require": "./dist/lib/index.browser.js",
        "import": "./dist/es/index.browser.mjs"
      },
      "require": "./dist/lib/index.js",
      "import": "./dist/es/index.mjs"
    }
  },
  "files": ["dist", "static"],
  "scripts": {
    "build": "rslib build",
    "build:watch": "rslib build --watch",
    "dev": "rslib build --watch",
    "test": "vitest --run",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@midscene/core": "workspace:*",
    "@midscene/shared": "workspace:*",
    "dotenv": "16.4.5",
    "express": "^4.21.2",
    "open": "10.1.0"
  },
  "devDependencies": {
    "@rslib/core": "^0.11.2",
    "@types/express": "^4.17.21",
    "@types/node": "^18.0.0",
    "typescript": "^5.8.3",
    "vitest": "3.0.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
