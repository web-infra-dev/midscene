  },
  "dependencies": {
    "@midscene/ios": "workspace:*"
  },
  "devDependencies": {
    "@rslib/core": "^0.18.3",
    "@types/node": "^18.0.0",
    "typescript": "^5.8.3"
  },
  "license": "MIT"
}
