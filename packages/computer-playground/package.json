    "build:watch": "rslib build --watch --no-clean"
  },
  "dependencies": {
    "@midscene/computer": "workspace:*",
    "@midscene/core": "workspace:*",
    "@midscene/playground": "workspace:*",
    "@midscene/shared": "workspace:*",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "puppeteer": "24.6.0"
  },
  "devDependencies": {
    "@rslib/core": "^0.18.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^18.0.0",
    "typescript": "^5.8.3"
  },
  "license": "MIT"
}
