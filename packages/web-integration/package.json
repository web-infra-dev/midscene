    "*": {
      ".": ["./dist/types/index.d.ts"],
      "bridge-mode": ["./dist/types/bridge-mode.d.ts"],
      "bridge-mode-browser": ["./dist/types/bridge-mode-browser.d.ts"],
      "utils": ["./dist/types/utils.d.ts"],
      "ui-utils": ["./dist/types/ui-utils.d.ts"],
