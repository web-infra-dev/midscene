{
  "name": "@midscene/web-mcp",
  "version": "1.0.0",
  "description": "Midscene MCP Server for Web automation (alias for @midscene/mcp)",
  "bin": "dist/index.js",
  "files": ["dist"],
  "main": "./dist/server.js",
  "types": "./dist/server.d.ts",
  "exports": {
    ".": {
      "types": "./dist/server.d.ts",
      "default": "./dist/server.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "default": "./dist/server.js"
    }
  },
  "scripts": {
    "build": "rslib build",
    "dev": "npm run build:watch",
    "build:watch": "rslib build --watch",
    "mcp-playground": "npx @modelcontextprotocol/inspector node ./dist/index.js",
    "test": "vitest run",
    "inspect": "node scripts/inspect.mjs"
  },
  "dependencies": {
    "@midscene/mcp": "workspace:*"
  },
  "devDependencies": {
    "@rslib/core": "^0.11.2",
    "@types/node": "^18.0.0",
    "typescript": "^5.8.3",
    "vitest": "3.0.5"
  },
  "license": "MIT"
}
