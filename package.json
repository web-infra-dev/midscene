    "node": ">=18.19.0"
  },
  "packageManager": "pnpm@9.3.0",
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "eslint": "^8.57.0",
    "@modern-js/eslint-config": "^2.54.2",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "pretty-quick": "3.1.3",
    "@changesets/cli": "2.24.1",
    "commitizen": "4.2.5",
    "nx": "19.5.2",
    "semver": "7.3.7",
    "dayjs": "1.11.2",
    "minimist": "1.2.5",
    "execa": "4.1.0",
    "chalk": "4.1.2",
